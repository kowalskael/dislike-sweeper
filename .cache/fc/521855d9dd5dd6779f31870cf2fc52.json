n}\n\n.field > input {\n  display: flex;\n  font: 1em \"Roboto\", sans-serif;\n  height: 40px;\n  width: 35px;\n}\n\ninput[type=number]::-webkit-inner-spin-button,\ninput[type=number]::-webkit-outer-spin-button {\n  -webkit-appearance: none;\n  margin: 0;\n}\n\n.field > input[value] {\n  text-align: center;\n  border: none;\n  font: 1em \"Roboto\", sans-serif;\n}\n\n.field > label {\n  font-size: 15px;\n  padding: 0px 18px 0px 18px;\n  width: 155px;\n  color: black;\n}\n\n.changeValue {\n  color: black;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  width: 25px;\n}\n\n.arrow-up {\n  transform: rotate(-90deg);\n}\n\n.arrow-down {\n  transform: rotate(90deg);\n}\n\n.arrow-right {\n  margin-left: 3px;\n}\n\n.button {\n  display: flex;\n  align-items: center;\n  justify-content: flex-end;\n}\n\n#submit, #reset {\n  margin-top: 4px;\n  background-color: transparent;\n  border: none;\n  display:inline-block;\n  cursor:pointer;\n  font: 1em \"Roboto\", sans-serif, medium;\n  font-weight: 500;\n  font-stretch: 200%;\n  font-kerning: normal;\n  color: #525252;\n  text-decoration:none;\n}\n\n#submit:hover, #reset:hover {\n  background-color: white;\n  border: none;\n  color: #2f55a4;\n}\n\n#play {\n  flex-direction: column;\n  padding: 5px 50px 5px 50px;\n}\n\n#play > div {\n  display: flex;\n}\n\n#timer, #reset {\n  margin: 15px 0px 12px 0px;\n}\n\n#timer {\n  color: black;\n  font-size: 18px;\n  align-items: flex-start;\n  margin-left: 10px;\n}\n\n.timer {\n  align-items: center;\n}\n\n#board {\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  font-size: 0.9em;\n  color: white;\n  text-align: center;\n  line-height: 23px;\n  vertical-align: middle;\n}\n\n#board > div {\n  display: flex;\n  flex-direction: row;\n  height: 25px;\n  width: 100%;\n}\n\n#board > div > div {\n  justify-content: center;\n  width: 25px;\n  height: 25px;\n}\n\n.play {\n  border-width: 1px;\n  border-style: solid;\n  border-color: #525252;\n  box-sizing: border-box;\n}\n\n.lose {\n  border-style: solid;\n  border-color: #2f55a4;\n  border-width: 1px;\n  box-sizing: border-box;\n}\n\n.hidden {\n  background: #ffd445;\n}\n\n.revealed {\n\n}\n\n.flagged {\n  background: #ff5719;\n}\n\n.revealed:before {\n  content: attr(data-value);\n}\n\n.revealed[data-value=\"0\"]:before {\n  content: '';\n}\n\n.revealed[data-value=\"0\"] {\n  background: #ffffff;\n}\n\n.revealed[data-value=\"1\"] {\n  background: #598fff;\n}\n\n.revealed[data-value=\"2\"] {\n  background: #3d7cff;\n}\n\n.revealed[data-value=\"3\"] {\n  background: #296eff;\n}\n\n.revealed[data-value=\"4\"] {\n  background: #0d5afc;\n}\n\n.revealed[data-value=\"5\"] {\n  background: #0045d6;\n}\n\n.revealed[data-value=\"6\"] {\n  background: #254485;\n}\n\n.revealed[data-value=\"7\"] {\n  background: #193570;\n}\n\n.revealed[data-value=\"8\"] {\n  background: #0d2659;\n}\n\n.revealed[data-value=\"9\"] {\n  background: #ffffff;\n}\n\n.revealed[data-value=\"9\"]:before {\n  content: url('icon/smile.svg');\n}\n\n.win {\n  background: #ffd445;\n  border-style: solid;\n  border-color: #2f55a4;\n  border-width: 1px;\n  box-sizing: border-box;\n}\n\n.win[data-value=\"9\"]:before {\n  content: url('icon/dislike.svg');\n}\n\n\n"},"lineCount":null}},"error":null,"hash":"6bb3f21e0b40bdd215f04a9f2e672036","cacheData":{"env":{}}}